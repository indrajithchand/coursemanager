@model HM.CourseManagement.Web.Models.CourseList

<div class="container">
    <h1>Contents</h1>
    <hr />
    <br />
    <table class="table table-striped table-bordered dt-responsive">
        <tr>
            <td valign="top"><b>@Html.DisplayNameFor(model => model.Course.Name):</b></td>
            <td>
                @Html.DisplayFor(model => model.Course.Name)
            </td>
        </tr>
        <tr>
            <td valign="top"><b>@Html.DisplayNameFor(model => model.Schedule):</b></td>
            <td>
                @Html.DisplayFor(model => model.Schedule)
            </td>
        </tr>
        <tr>
            <td valign="top"><b>@Html.DisplayNameFor(model => model.Location):</b></td>
            <td>
                @Html.DisplayFor(model => model.Location)
            </td>
        </tr>

        <tr>
            <td valign="top"><b>@Html.DisplayNameFor(model => model.Faculty):</b></td>
            <td>
                @Html.DisplayFor(model => model.Faculty.Name)
            </td>
        </tr>
    </table>
</div>
